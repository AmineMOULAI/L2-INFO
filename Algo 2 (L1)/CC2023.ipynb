    "def est_palindrome_nospace(s : str) -> bool:\n",
    "    i = 0\n",
    "    j = len(s) - 1\n",
    "    while i < j:\n",
    "        if s[i] != \" \" and s[j] != \" \":\n",
    "            if s[i] != s[j]:\n",
    "                return False\n",
    "            i += 1\n",
    "            j -= 1\n",
    "        else:\n",
    "            if s[i] == \" \" and s[j] == \" \":\n",
    "                i += 1\n",
    "                j -= 1\n",
    "            elif s[i] == \" \":\n",
    "                i += 1\n",
    "            else:\n",
    "                j -= 1\n",
    "    return True"
   ]
